{
  "name": "@starter/db",
  "type": "module",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.mjs"
    },
    "./schema": {
      "types": "./dist/schema/index.d.mts",
      "import": "./dist/schema/index.mjs",
      "default": "./dist/schema/index.mjs"
    },
    "./schema/auth": {
      "types": "./dist/schema/auth.d.mts",
      "import": "./dist/schema/auth.mjs",
      "default": "./dist/schema/auth.mjs"
    },
    "./schema/issue": {
      "types": "./dist/schema/issue.d.mts",
      "import": "./dist/schema/issue.mjs",
      "default": "./dist/schema/issue.mjs"
    }
  },
  "scripts": {
    "build": "tsdown",
    "type-check": "tsc -b",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate"
  },
  "devDependencies": {
    "drizzle-kit": "0.31.8",
    "@types/ws": "8.18.1",
    "@starter/config": "workspace:*",
    "tsdown": "0.20.1"
  },
  "peerDependencies": {
    "typescript": "5.9.3"
  },
  "dependencies": {
    "@neondatabase/serverless": "1.0.2",
    "dotenv": "17.2.3",
    "drizzle-orm": "0.45.1",
    "postgres": "3.4.8",
    "ws": "8.19.0",
    "zod": "4.3.6"
  }
}
