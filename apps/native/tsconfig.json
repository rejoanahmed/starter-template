{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@api/*": ["../api/src/*"],
      "@app/*": ["./*"],
      "tailwind.config": ["./tailwind.config.js"]
    },
    "strictNullChecks": true
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts",
    "../api/worker-configuration.d.ts"
  ]
}
